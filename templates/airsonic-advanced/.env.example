# Environment variables for Airsonic-advanced

# PUID
# for GroupID
PUID=1024

# PGID
# for UserID
PGID=100

# TZ
# Specify a timezone to use for example Europe/Amsterdam
TZ=Europe/Amsterdam

# CONTEXT_PATH
# For setting url-base in reverse proxy setups.
CONTEXT_PATH=<URL_BASE>

# JAVA_OPTS
# For passing additional java options.
JAVA_OPTS=<options>

