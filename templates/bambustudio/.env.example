# Environment variables for Bambustudio

# PUID
# for UserID
PUID=1024

# PGID
# for GroupID
PGID=100

# TZ
# specify a timezone to use, see this [list](https://en.wikipedia.org/wiki/List_of_tz_database_time_zones#List).
TZ=Europe/Amsterdam

# DARK_MODE
# Set this to true to enable dark mode for Bambu Studio.
DARK_MODE=true

